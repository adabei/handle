---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Handle</title>
 <link href="http://www2.htlwrn.ac.at/handle/atom.xml" rel="self"/>
 <link href="http://www2.htlwrn.ac.at/handle/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www2.htlwrn.ac.at/handle/</id>
 <author>
   <name>Handle Project</name>
   <email>handle@htlwrn.ac.at</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www2.htlwrn.ac.at/handle{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www2.htlwrn.ac.at/handle{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
